{
  "name": "ad/youtube",
  "description": "Youtube API3 Package for laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Ajit Das",
      "email": "ajit@madgeek.in"
    }
  ],
  "require": {
    "php": "^5.6|^7.1|^8.0",
    "illuminate/support": "^5.4|^6.0|^7.0|^8.0",
    "google/apiclient": "^2.2",
    "nesbot/carbon": "^2.17"
  },
  "require-dev": {
    "orchestra/testbench": "~3.2",
    "phpunit/phpunit": "~5.0",
    "phpunit/phpcov": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "ad\\Youtube\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ad\\Youtube\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "ad\\Youtube\\YoutubeAPIServiceProvider"
      ],
      "aliases": {
        "Youtube": "ad\\Youtube\\Facades\\YoutubeAPI"
      }
    }
  }
}